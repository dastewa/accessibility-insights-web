{
    "name": "accessibility-insights-web",
    "version": "1.0.0",
    "description": "Accessibility Insights for Web",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/Microsoft/accessibility-insights-web"
    },
    "engines": {
        "node": ">=10.16.0",
        "yarn": "^1.17.3"
    },
    "scripts": {
        "build": "grunt",
        "build:all": "grunt build-all",
        "build:dev": "grunt build-dev",
        "build:electron": "grunt build-electron",
        "build:prod": "grunt build-prod",
        "copyrightheaders": "grunt copyright-check",
        "copyrightheaders:fix": "grunt copyright-add",
        "dist:electron": "yarn build:electron && electron-builder -p never",
        "format": "prettier --config prettier.config.js --write \"**/*\"",
        "format-check": "prettier --config prettier.config.js --check \"**/*\"",
        "generate-scss-typings": "tsm \"src/**/*.scss\"",
        "clean:scss-typings": "grunt clean:scss",
        "precheckin": "npm-run-all --serial clean:scss-typings format-check tslint copyrightheaders build:all test test:e2e",
        "start:electron": "electron drop/electron/extension/bundle/main.bundle.js",
        "test": "jest --projects src/tests/unit",
        "test:e2e": "jest --projects src/tests/end-to-end --runInBand",
        "test:e2e:docker:build": "docker build -t accessibility-insights-web-e2e .",
        "test:e2e:docker:run": "docker run -t accessibility-insights-web-e2e",
        "test:e2e:docker": "npm-run-all --serial test:e2e:docker:build \"test:e2e:docker:run {@}\" --",
        "test:electron": "jest --projects src/tests/electron --runInBand",
        "tslint": "tslint -c ./tslint.json -p ./tsconfig.json",
        "tslint:fix": "tslint --fix -c ./tslint.json -p ./tsconfig.json",
        "watch": "npm-run-all --parallel --race --print-label watch:scss watch:grunt watch:test watch:webpack",
        "watch:build": "npm-run-all --parallel --race --print-label watch:scss watch:grunt watch:webpack",
        "watch:scss": "tsm src/**/*.scss --watch",
        "watch:grunt": "grunt watch",
        "watch:test": "jest --watch --coverage false --colors",
        "watch:webpack": "webpack --watch --config-name dev --colors --progress"
    },
    "devDependencies": {
        "@types/applicationinsights-js": "^1.0.7",
        "@types/chrome": "0.0.88",
        "@types/enzyme": "^3.10.3",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/jest": "^24.0.18",
        "@types/jsdom": "^12.2.4",
        "@types/lodash": "^4.14.138",
        "@types/make-dir": "^2.1.0",
        "@types/moment": "^2.13.0",
        "@types/puppeteer": "^1.19.1",
        "@types/q": "^1.5.2",
        "@types/react": "^16.9.2",
        "@types/react-copy-to-clipboard": "^4.2.6",
        "@types/react-dom": "^16.9.0",
        "@types/react-helmet": "^5.0.10",
        "@types/react-router-dom": "^4.3.5",
        "@types/serve-static": "^1.13.3",
        "@types/ua-parser-js": "^0.7.33",
        "@types/uuid": "^3.4.5",
        "case-sensitive-paths-webpack-plugin": "^2.2.0",
        "core-js-bundle": "^3.2.1",
        "css-loader": "^3.2.0",
        "electron": "^6.0.9",
        "electron-builder": "^21.2.0",
        "enzyme": "^3.10.0",
        "enzyme-adapter-react-16": "^1.14.0",
        "express": "^4.16.4",
        "fake-indexeddb": "^2.1.1",
        "fork-ts-checker-webpack-plugin": "^1.5.0",
        "grunt": "^1.0.4",
        "grunt-bom-removal": "1.0.1",
        "grunt-cli": "^1.3.1",
        "grunt-concurrent": "^3.0.0",
        "grunt-contrib-clean": "^2.0.0",
        "grunt-contrib-copy": "^1.0.0",
        "grunt-contrib-watch": "^1.1.0",
        "grunt-exec": "^3.0.0",
        "grunt-sass": "^3.1.0",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^24.9.0",
        "jest-circus": "^24.9.0",
        "jest-junit": "^8.0.0",
        "license-check-and-add": "2.3.6",
        "make-dir": "^3.0.0",
        "mini-css-extract-plugin": "0.8.0",
        "node-sass": "^4.12.0",
        "npm-run-all": "^4.1.5",
        "preprocess": "^3.1.0",
        "prettier": "^1.18.0",
        "puppeteer": "^1.20.0",
        "regenerator-runtime": "^0.13.3",
        "sass-loader": "^8.0.0",
        "script-loader": "0.7.2",
        "serve-static": "^1.13.2",
        "source-map-loader": "^0.2.4",
        "spectron": "^8.0.0",
        "terser-webpack-plugin": "^2.0.1",
        "ts-jest": "^24.0.2",
        "ts-loader": "^6.1.0",
        "tslint": "^5.20.0",
        "tslint-microsoft-contrib": "6.2.0",
        "typed-scss-modules": "^0.0.12",
        "typemoq": "^2.1.0",
        "typescript": "^3.6.3",
        "webdriverio": "^4.13.0",
        "webpack": "^4.39.3",
        "webpack-cli": "^3.3.8"
    },
    "dependencies": {
        "applicationinsights-js": "^1.0.20",
        "axe-core": "3.2.2",
        "axios": "^0.19.0",
        "classnames": "^2.2.6",
        "idb-keyval": "^3.2.0",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "office-ui-fabric-react": "6.200.1",
        "q": "1.5.1",
        "react": "^16.9.0",
        "react-copy-to-clipboard": "^5.0.1",
        "react-dom": "^16.9.0",
        "react-helmet": "^5.2.0",
        "react-router-dom": "^5.0.1",
        "ua-parser-js": "^0.7.20",
        "uuid": "^3.3.3"
    }
}
